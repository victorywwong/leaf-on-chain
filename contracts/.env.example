# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Treasury address to receive platform fees (30%)
TREASURY_ADDRESS=0xYourTreasuryAddressHere

# Base Sepolia RPC URL (default is public, but you can use Alchemy/Infura)
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# BaseScan API key for contract verification (optional)
BASESCAN_API_KEY=your_basescan_api_key_here
